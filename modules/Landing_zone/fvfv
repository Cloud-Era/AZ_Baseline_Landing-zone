# Swimlane UDRs
swimlane_udrs = {
  for udr_name, rt_name in var.swimlane_udr_names : udr_name => {
    extra_templates = []
    variables = {}
    name = rt_name
  }
}
variables.tf
hcl
Copy code
variable "swimlane_udr_names" {
  description = "Map of Swimlane UDR names"
  type        = map(string)
}
